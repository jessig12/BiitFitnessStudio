<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <link rel="stylesheet" href="quejas.css">
    <link rel="stylesheet" href="/global.css">
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.0/css/all.min.css">
    <title>Quejas y sugerencias</title>
</head>
<body>
    <!--navbar-->
    <%- include('./partials/_header'); %>
    <!--linea roja-->
    <div class="navbar-after">
        <a class="after"></a class></a>
    </div>
    <div class="container-fluid rectangulo" id="sec1">
        <div class="container my-5 rectangulo">
            <h1 class="titulo">QUEJAS Y SUGERENCIAS </h1>
        </div>
    </div>
    <!--linea roja-->
    <div class="navbar-after">
        <a class="after"></a class></a>
    </div>
    <!--formulario-->
    
    <div class="body2" id="formularioPie">
        <form method="post" id="biit" class="registro" onsubmit="event.preventDefault(); validateForm()">

        <div class="container w-50 h-50 " id="formulario">
            <div class="mb-3">
                <label for="email" class="form-label">Email</label>
                <input type="email" class="form-control" id="email" placeholder="name@example.com">
                <small class="error"></small>
            </div>
            <div class="mb-3">
                <label for="asunto" class="form-label">Asunto</label>
                <textarea id="subject" class="form-control" placeholder="Asunto"></textarea>
                <small class="error"></small>
            </div>
            <div class="mb-3">
                <label for="message" class="form-label">Queja y sugerencia</label>
                <textarea class="form-control" id="message" placeholder="Escriba aqui su queja o sugerencia"rows="3"></textarea>
                <small class="error"></small>

            </div>
            <div class="mb-3 form-check">
                <input type="checkbox" class="form-check-input" id="gridCheck">
                <label class="form-check-label" for="gridCheck">No soy un robot</label>
              </div>
              <button id="boton" type="submit" class="btn btn-danger btn-lg justify-content-center" >Enviar</button>
              <p id="success"></p>
            </form>
        </div>
    

    </div>
    <!--linea roja-->
    <div class="navbar-after">
      <a class="after"></a class></a>
  </div>
  <%- include('./partials/_footer'); %>
  <script src="/quejas.js"></script>

</body>
</html>